    "input_kalimat = input(\"masukkan kalimat: \")\n",
    "def buat_akronim(kalimat):\n",
    "   \n",
    "    kata_kata = kalimat.split()\n",
    "   \n",
    "    akronim = ''.join(kata[0].upper() for kata in kata_kata)\n",
    "    return akronim\n",
    "\n",
    "buat_akronim(input_kalimat)"
